{
    "name": "tupy/fulltextsearch",
    "description": "Laravel Full Text Search",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aguinaldo Tupy",
            "email": "aguinaldo.tupy@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Tupy\\FullTextSearch\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Tupy\\FullTextSearch\\FullTextSearchServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}